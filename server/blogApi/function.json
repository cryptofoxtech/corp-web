{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get",
        "post"
      ],
      "route": "blogApi" // Explicitly define the route
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    }
  ],
  "script": "index.cjs"
}